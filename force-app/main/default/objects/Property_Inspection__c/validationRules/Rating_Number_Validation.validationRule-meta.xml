<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Number_Validation</fullName>
    <active>true</active>
    <description>Must between 1-5</description>
    <errorConditionFormula>OR(Overall_Rating__c &lt; 1,Overall_Rating__c &gt; 5)</errorConditionFormula>
    <errorMessage>Rating must between 1-5</errorMessage>
</ValidationRule>
